{
  "client": {
    "app": {
      "version" : "0.0.1",
      "updateInterval": "1 hours",
      "mountpath": "/wilson"
    },
    "i18n": {
      "lng": "en",
      "fallbackLng": "en",
      "defaultLng": "en",
      "supportedLngs": ["en"],
      "namespaces": ["app"],
      "sendMissing": true,
      "useLocalStorage": false,
      "localStorageExpirationTime": 604800000
    },
    "cdn": false
  },
  "server" : {
    "deploy": {
      "mode": "development",
      "logLevel": "debug",
      "path": "."
    },
    "projectPaths": {
      "root": "/Users/justin.fiedler/Documents/dev/Wilson/wilson-framework/app",
      "components": "client/src/components",
      "services": "client/src/services",
      "behaviors": "client/src/behaviors",
      "parsers": "client/src/services/parsers",
      "utilities": "client/src/services/utilities",
      "filters": "client/src/filters",
      "guides":  "client/src/guides",
      "tagMiddleware": "server/tag-middleware",
      "routes": "client/routing.json",
      "IRouteService": "client/src/services/IRouteService.js"
    },
    "dependencies": {
      "ignored": ["localStorageService"],
      "core": [
        "AppStateService",
        "BusFactoryService",
        "ComponentFactoryService",
        "ComponentLoaderService",
        "i18nextService",
        "ParserFactoryService",
        "ResourceLoaderService",
        "StateMachineService"
      ]
    },
    "tags": {
      "brand":       10,
      "device":       9,
      "platform":     8,
      "account":      7,
      "language":     6,
      "locale":       5,
      "pref-locale":  4
    },
    "geo": {
      "implyTimezone": false
    },
    "caching": {
      "useServerCache": false,
      "folder": "server/.cache",
      "maxAge": {
        "assets":     1000,
        "appShell":   1000,
        "components": 1000,
        "bundles":    1000
      }
    }
  }
}
{
  "client": {
    "app": {
      "version" : "0.0.0",
      "updateIntervalMillis": 3600000,
      "mountpath": "/wilson",
      "autoLocalizeRoutes": true
    },
    "i18n": {
      "lng": "en",
      "fallbackLng": "en",
      "defaultLng": "en",
      "supportedLngs": ["en"],
      "namespaces": [],
      "sendMissing": true,
      "useLocalStorage": false,
      "localStorageExpirationTime": 604800000,
      "clientSafeNamespaces": "all"
    },
    "cdn": false,
    "prerender": {
      "enabled":    false,
      "enableLogs": false
    },
    "debugInfoEnabled": false,
    "defaultCtrlAs": false
  },
  "server" : {
    "deploy": {
      "mode": "development",
      "logLevel": "debug",
      "path": "."
    },
    "projectPaths": {
      "root": ".",
      "clientApp": "/client/app.js",
      "components": "/client/src/components",
      "behaviors": "/client/src/behaviors",
      "services": "/client/src/services",
      "parsers": "/client/src/services/parsers",
      "resources": "/client/src/services/resources",
      "utilities": "/client/src/services/utilities",
      "filters": "/client/src/filters",
      "guides":  "/client/src/guides",
      "tagMiddleware": "/server/tag-middleware",
      "locales": "/client/locales",
      "routes": "/client/routing.json",
      "routeService": "/client/router.js"
    },
    "dependencies": {
      "ignored": ["localStorageService"],
      "core": [
        "AppStateService",
        "ComponentFactoryService",
        "ComponentLoaderService",
        "i18nextService",
        "ResourceLoaderService",
        "StateMachineService",
        "ComponentPrerenderService",
        "DeprecatedFunctionService"
      ],
      "selectors": {
        "component": "ht",
        "behavior": "ht",
        "guide": "guide"
      }
    },
    "tags": {
      "language":     6,
      "locale":       5,
      "pref-locale":  4
    },
    "geo": {
      "implyTimezone": false
    },
    "caching": {
      "useServerCache": false,
      "readChangeList": false,
      "folder": "server/.cache",
      "maxAge": {
        "assets":     1000,
        "appShell":   1000,
        "components": 1000,
        "bundles":    1000,
        "wilson":     1000
      }
    }
  }
}